	animation-iteration-count:1;
	animation-play-state:paused;
	animation-fill-mode:forwards;
}

@keyframes abcd{
	0%{
		bottom:-50px;
	}
	20%{
		bottom:20px;
	}
	80%{
		bottom:20px;
	}
	100%{
		bottom:-50px;
		
	}
}

